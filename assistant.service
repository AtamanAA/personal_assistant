[Unit]
Description=Personal assistant uvicorn run service
After=network.target
[Service]
User=assistant
Group=assistant
WorkingDirectory=/home/assistant/personal_assistant/personal_assistant
ExecStart=/home/assistant/personal_assistant/venv/bin/uvicorn personal_assistant.main:app --host 0.0.0.0 --port 8004
[Install]
WantedBy=multi-user.target